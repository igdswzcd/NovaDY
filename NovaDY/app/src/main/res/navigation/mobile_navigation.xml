<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_videos">

    <fragment
        android:id="@+id/navigation_videos"
        android:name="com.zxj.novady.videos.VideosFragment"
        android:label="@string/title_videos"
        tools:layout="@layout/fragment_videos" />

    <fragment
        android:id="@+id/navigation_localvideos"
        android:name="com.zxj.novady.localvideos.LocalvideosFragment"
        android:label="@string/title_localvideos"
        tools:layout="@layout/fragment_localvideos" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.zxj.novady.message.MessageFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_message" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.zxj.novady.settings.SetFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />

</navigation>